 #include <stdio.h>
#include <math.h>

#define LATA_TINTA 2000
#define CONSUMO_TINTA 300

int main() {
  float largura, altura, area, quantidade_latas;

  printf("Digite a largura (em metros): ");
  scanf("%f", &largura);

  printf("Digite a altura (em metros): ");
  scanf("%f", &altura);

  area = largura * altura; 
  quantidade_latas = ceil(area * CONSUMO_TINTA / LATA_TINTA); 

  printf("Serão necessárias %.0f latas \n", quantidade_latas);

  return 0;
}
