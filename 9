#include <iostream>

int main() {
  
  float n1, n2, n3, mediafinal;

  printf("Digite a nota 1 \n");
  scanf("%f", &n1);

   printf("Digite a nota 2  \n");
  scanf("%f", &n2);

   printf("Digite a nota 3  \n");
  scanf("%f", &n3);

  mediafinal = ((n1*2) + (n2*3) + (n3*5)) /10;
    
  printf("a media final e %f \n", mediafinal);
  
}
